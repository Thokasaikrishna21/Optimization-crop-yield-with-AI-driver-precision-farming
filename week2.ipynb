import zipfile
import pandas as pd
import os

# Path to the uploaded ZIP file
zip_path = "archive.zip"

# 1️⃣ Extract the ZIP file
with zipfile.ZipFile(zip_path, 'r') as zip_ref:
    zip_ref.extractall("data")

# 2️⃣ Check extracted files
data_dir = "data"
print("Extracted files:", os.listdir(data_dir))

# 3️⃣ Load the CSV file(s)
# Adjust the filename based on what gets printed above
csv_path = os.path.join(data_dir, "your_dataset_name.csv")  # Replace with actual name
df = pd.read_csv(csv_path)

# 4️⃣ View dataset info
print(df.head())
print(df.info())
